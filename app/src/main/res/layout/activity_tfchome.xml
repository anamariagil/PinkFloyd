            android:id="@+id/tootf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/bootf"
            app:layout_constraintStart_toStartOf="@id/bootf"
            app:layout_constraintTop_toTopOf="@id/bootf"
            app:layout_constraintBottom_toBottomOf="@id/bootf"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="17dp"
            android:fontFamily="monospace"
            android:text="One Of The Few"
