            app:layout_constraintEnd_toEndOf="@id/bnnj"
            app:layout_constraintStart_toStartOf="@id/bnnj"
            app:layout_constraintTop_toTopOf="@id/bnnj"
            app:layout_constraintBottom_toBottomOf="@id/bnnj"
            android:textAlignment="center"
