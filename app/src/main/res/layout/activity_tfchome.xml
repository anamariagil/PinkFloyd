            app:layout_constraintTop_toTopOf="@id/bthr"
            app:layout_constraintBottom_toBottomOf="@id/bthr"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="13dp"
            android:fontFamily="monospace"
            android:text="The Hero's Return"
            />

