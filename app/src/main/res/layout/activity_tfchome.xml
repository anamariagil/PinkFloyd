            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/bthr"
            android:layout_marginTop="15dp"
            />

        <TextView
            android:id="@+id/ttgd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="@id/btgd"
            app:layout_constraintStart_toStartOf="@id/btgd"
            app:layout_constraintTop_toTopOf="@id/btgd"
            app:layout_constraintBottom_toBottomOf="@id/btgd"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="13dp"
            android:fontFamily="monospace"
            android:text="The Gunner's Dream"
            />

        <ImageButton
            android:id="@+id/bpe"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/btgd"
